{
  "name": "libp2p-pstn-logger",
  "version": "1.0.4",
  "description": "A data logger for benchmarking libp2p pubsub implementations",
  "main": "src/index.js",
  "scripts": {
    "test": "DEBUG=-* mocha -t 60000 test/**/*.spec.js",
    "test:debug": "DEBUG=pstn:logger* mocha -t 60000 test/**/*.spec.js",
    "test:log": "DEBUG_FD=2 DEBUG=pstn:logger* mocha -t 60000 test/**/*.spec.js 2> pstn.log"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gavinmcdermott/js-libp2p-pstn-logger.git"
  },
  "keywords": [
    "pubsub",
    "ipfs",
    "swarm",
    "libp2p",
    "messaging",
    "benchmark",
    "logging"
  ],
  "author": "Gavin McDermott <gavinmcdermott@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gavinmcdermott/js-libp2p-pstn-logger/issues"
  },
  "homepage": "https://github.com/gavinmcdermott/js-libp2p-pstn-logger#readme",
  "dependencies": {
    "debug": "2.2.0",
    "libp2p-pstn-node": "1.0.5",
    "ramda": "0.22.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "libp2p-floodsub": "0.3.0"
  }
}
